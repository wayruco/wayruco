{
  "private": true,
  "packageManager": "pnpm@8.0.0",
  "scripts": {
    "dev": "turbo run dev",
    "dev:landing": "turbo run dev --filter=@wayruco/landing",
    "dev:all": "concurrently \"npm run dev:landing\"",
    "dev:direct": "cd apps/landing && npm run dev",
    "build": "turbo run build",
    "build:landing": "turbo run build --filter=@wayruco/landing",
    "build:packages": "turbo run build --filter=./packages/*",
    "build:apps": "turbo run build --filter=./apps/*",
    "build:contracts": "turbo run build --filter=./contracts/*",
    "start": "turbo run start",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:integration": "turbo run test:integration",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "type-check": "turbo run type-check",
    "version": "node -e \"const fs=require('fs');const v=JSON.parse(fs.readFileSync('version.json','utf8'));console.log(\\`v\\${v.version} (\\${v.buildNumber})\\`);\"",
    "version:patch": "npm run version:bump patch",
    "version:minor": "npm run version:bump minor",
    "version:major": "npm run version:bump major",
    "version:release": "npm run version:bump patch --release-tag",
    "version:release-patch": "npm run version:bump patch --release-tag",
    "version:release-minor": "npm run version:bump minor --release-tag",
    "version:release-major": "npm run version:bump major --release-tag",
    "version:bump": "node scripts/bump-version.js",
    "changelog": "node scripts/update-changelog.js",
    "check-build": "node scripts/check-build.js",
    "prebuild": "npm run check-build",
    "release": "npm run version:bump && npm run changelog && npm run check-build && npm run build"
  },
  "dependencies": {
