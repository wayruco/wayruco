    "input-otp": "^1.4.2",
    "lucide-react": "^0.468.0",
    "next": "14.2.23",
    "next-themes": "^0.2.1",
    "prettier": "^3.3.3",
    "radix-ui": "^1.1.3",
    "react": "^18",
    "react-day-picker": "^9.12.0",
    "react-dom": "^18",
    "react-hook-form": "^7.68.0",
    "react-resizable-panels": "^2.1.9",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
